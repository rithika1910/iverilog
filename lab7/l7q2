:vpi_module "va_math";
S_0000000001026c90 .scope module, "forever_example" "forever_example" 2 1;
 .timescale 0 0;
v00000000010269a0_0 .var "clk", 0 0;
    .scope S_0000000001026c90;
T_0 ;
    %delay 1, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v00000000010269a0_0, 0, 1;
T_0.0 ;
    %delay 5, 0;
    %load/vec4 v00000000010269a0_0;
    %nor/r;
    %store/vec4 v00000000010269a0_0, 0, 1;
    %jmp T_0.0;
    %end;
    .thread T_0;
    .scope S_0000000001026c90;
T_1 ;
    %vpi_call 2 10 "$monitor", "Time =%d  clk =%b", $time, v00000000010269a0_0 {0 0 0};
    %delay 100, 0;
    %vpi_call 2 11 "$finish" {0 0 0};
    %end;
    .thread T_1;
