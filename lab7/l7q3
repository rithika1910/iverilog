:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_00000000010e6520 .scope module, "repeat_example" "repeat_example" 2 1;
 .timescale 0 0;
v00000000010e66a0_0 .var "data", 15 0;
v00000000010eef30_0 .var "opcode", 3 0;
v00000000010e6740_0 .var "temp", 0 0;
E_00000000010ebc80 .event edge, v00000000010e66a0_0, v00000000010eef30_0;
    .scope S_00000000010e6520;
T_0 ;
    %wait E_00000000010ebc80;
    %load/vec4 v00000000010eef30_0;
    %pad/u 32;
    %cmpi/e 10, 0, 32;
    %jmp/0xz  T_0.0, 4;
    %pushi/vec4 8, 0, 32;
T_0.2 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_0.3, 5;
    %jmp/1 T_0.3, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %delay 1, 0;
    %load/vec4 v00000000010e66a0_0;
    %parti/s 1, 15, 5;
    %store/vec4 v00000000010e6740_0, 0, 1;
    %load/vec4 v00000000010e66a0_0;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %shiftl 4;
    %store/vec4 v00000000010e66a0_0, 0, 16;
    %load/vec4 v00000000010e6740_0;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v00000000010e66a0_0, 4, 1;
    %jmp T_0.2;
T_0.3 ;
    %pop/vec4 1;
T_0.0 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_00000000010e6520;
T_1 ;
    %vpi_call 2 16 "$display", " TEMP DATA" {0 0 0};
    %vpi_call 2 17 "$monitor", " %b %b ", v00000000010e6740_0, v00000000010e66a0_0 {0 0 0};
    %delay 1, 0;
    %pushi/vec4 240, 0, 16;
    %store/vec4 v00000000010e66a0_0, 0, 16;
    %delay 1, 0;
    %pushi/vec4 10, 0, 4;
    %store/vec4 v00000000010eef30_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v00000000010eef30_0, 0, 4;
    %delay 1, 0;
    %vpi_call 2 21 "$finish" {0 0 0};
    %end;
    .thread T_1;
